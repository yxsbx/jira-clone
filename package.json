{
  "name": "jira-clone",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "npm run build:css && tsc",
    "build:css": "tailwindcss -m -i ./src/styles/style.css -o ./src/styles/app-compiled.css",
    "dev": "concurrently \"npm run dev:css\" \"vite\"",
    "dev:css": "tailwindcss -w -i ./src/styles/style.css -o ./src/styles/app-compiled.css",
    "type-check": "tsc --pretty",
    "lint": "eslint --ext ts,tsx src",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "test": "vitest run",
    "test-coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@radix-ui/themes": "^3.1.4",
    "tailwindcss-radix": "^3.0.5",
    "uuid": "^8.3.2",
    "vite": "^4.4.8",
    "vite-tsconfig-paths": "^3.5.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.6.0",
    "@typescript-eslint/parser": "^8.6.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^7.5.0",
    "eslint": "^9.11.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "^3.4.12",
    "typescript": "^5.5.3",
    "vite": "^5.4.1",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^0.30.0"
  }
}
